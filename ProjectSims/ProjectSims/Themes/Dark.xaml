<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa="http://schemas.fontawesome.io/icons/">

    <fa:ImageAwesome Icon="SunOutline" x:Key="SunIcon" Foreground="#FF687EA7"/>
    <fa:ImageAwesome Icon="WindowMinimize" x:Key="MinimizeIcon" Width="10" HorizontalAlignment="Center" Foreground="#FF687EA7"/>
    <fa:ImageAwesome Icon="WindowRestore" x:Key="RestoreIcon" Width="15" Foreground="#FF687EA7"/>
    <fa:ImageAwesome Icon="WindowClose" x:Key="CloseIcon" Width="20" Foreground="#FF687EA7"/>
    <SolidColorBrush Color="#002051"  x:Key="MainColor"></SolidColorBrush>
    <SolidColorBrush Color="#1c1c1c" x:Key="CenterBackground"></SolidColorBrush>
    <SolidColorBrush Color="#0c181f" x:Key="RightBackground"></SolidColorBrush>
    <SolidColorBrush Color="#353535" x:Key="StatusBar"></SolidColorBrush>
    <SolidColorBrush Color="#ebf5ff" x:Key="TextboxColor"></SolidColorBrush>
    <SolidColorBrush Color="#ffaeaeae" x:Key="ElementsColor"></SolidColorBrush>
    <SolidColorBrush Color="#FF687EA7" x:Key="DiffElementsColor"></SolidColorBrush>
    <SolidColorBrush Color="#1c1c1c" x:Key="CenterDarkColor"></SolidColorBrush>
    <SolidColorBrush Color="#242424" x:Key="UpperColor"></SolidColorBrush>

</ResourceDictionary>