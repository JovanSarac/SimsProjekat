<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa="http://schemas.fontawesome.io/icons/">

    <fa:ImageAwesome Icon="SunOutline" x:Key="SunIcon" Foreground="#FFA2D0DA"/>
    <fa:ImageAwesome Icon="WindowMinimize" x:Key="MinimizeIcon" Width="10" HorizontalAlignment="Center" Foreground="#FF687EA7"/>
    <fa:ImageAwesome Icon="WindowRestore" x:Key="RestoreIcon" Width="15" Foreground="#FF687EA7"/>
    <fa:ImageAwesome Icon="WindowClose" x:Key="CloseIcon" Width="20" Foreground="#FF687EA7"/>
    <SolidColorBrush Color="#FF02272F"  x:Key="MainColor"></SolidColorBrush>
    <SolidColorBrush Color="DarkSlateGray" x:Key="CenterBackground"></SolidColorBrush>
    <SolidColorBrush Color="#FFA2D0DA" x:Key="ElementsColor"></SolidColorBrush>
    <SolidColorBrush Color="#FFAEAEAE" x:Key="TextboxColor"></SolidColorBrush>
    <SolidColorBrush Color="#FF16333A" x:Key="CenterDarkColor"></SolidColorBrush>
    
    
</ResourceDictionary>