<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa="http://schemas.fontawesome.io/icons/">

    <fa:ImageAwesome Icon="MoonOutline" x:Key="MoonIcon" Foreground="#FF114642"/>
    <fa:ImageAwesome Icon="WindowMinimize" x:Key="MinimizeIcon"  Width="10" HorizontalAlignment="Center" Foreground="#FF114642"/>
    <fa:ImageAwesome Icon="WindowRestore" x:Key="RestoreIcon" Width="15" Foreground="#FF114642"/>
    <fa:ImageAwesome Icon="WindowClose" x:Key="CloseIcon" Width="20" Foreground="#FF114642"/>
    <SolidColorBrush  x:Key="MainColor" Color="#d8d8d8" ></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="CenterBackground"></SolidColorBrush>
    <SolidColorBrush Color="#ebf5ff" x:Key="RightBackground"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="TextboxColor"></SolidColorBrush>
    <SolidColorBrush Color="#001330" x:Key="ElementsColor"></SolidColorBrush>
    <SolidColorBrush Color="#001330" x:Key="DiffElementsColor"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="CenterDarkColor"></SolidColorBrush>
    <SolidColorBrush Color="#d8d8d8" x:Key="StatusBar"></SolidColorBrush>
    <SolidColorBrush Color="#f0f0f0" x:Key="UpperColor"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="ContrastColor"></SolidColorBrush>

</ResourceDictionary>